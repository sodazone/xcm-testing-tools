{
  "name": "xcm-test-tools",
  "packageManager": "yarn@3.6.3",
  "type": "module",
  "scripts": {
    "build": "substrate-exec-rimraf ./dist && substrate-exec-tsc && yarn sed:shebang",
    "sed:shebang": "shx sed -i \"s/\\/env ts-node/\\/env node/g\" ./dist/cli/*.js > /dev/null",
    "lint": "eslint --cache --fix --ext .ts src/"
  },
  "bin": {
    "assets": "./dist/cli/assets.js",
    "decode": "./dist/cli/decode.js",
    "transfer": "./dist/cli/transfer.js"
  },
  "devDependencies": {
    "@substrate/dev": "^0.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "chalk": "4.1.2",
    "eslint": "^8.50.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "@substrate/asset-transfer-api": "^0.1.0",
    "commander": "^11.0.0"
  }
}
